{
  "name": "warehouse",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@types/luxon": "^1.4.1",
    "@types/mocha": "^5.2.5",
    "@types/uuid": "^3.4.4",
    "cycle-scripts-one-fits-all": "7.1.1",
    "cyclejs-test-helpers": "^4.0.0",
    "husky": "^1.1.2",
    "lint-staged": "^7.3.0",
    "prettier": "^1.14.3",
    "snabbdom-looks-like": "^1.0.4",
    "snabbdom-pragma": "^2.7.0"
  },
  "scripts": {
    "format": "prettier --write '{src,test}/**/*.{js,jsx,ts,tsx}'",
    "start": "cycle-scripts start",
    "test": "cycle-scripts test",
    "build": "cycle-scripts build",
    "eject": "cycle-scripts eject",
    "clean": "cycle-scripts clean"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "nyc": {
    "instrument": false,
    "sourceMap": false,
    "include": [
      "src/components"
    ],
    "reporter": [
      "html",
      "text"
    ]
  },
  "mocha-webpack": {
    "include": [
      "src/**/*.test.{ts,tsx}"
    ]
  },
  "dependencies": {
    "@cycle/dom": "^22.0.0",
    "@cycle/history": "^7.0.0",
    "@cycle/isolate": "^4.1.0",
    "@cycle/run": "^5.1.0",
    "@cycle/state": "^1.0.0",
    "@cycle/time": "^0.16.0",
    "cyclejs-utils": "^4.0.2",
    "cyclic-router": "^5.1.7",
    "luxon": "^1.8.0",
    "switch-path": "^1.2.0",
    "uuid": "^3.3.2",
    "xstream": "^11.7.0"
  },
  "one-fits-all": true
}
